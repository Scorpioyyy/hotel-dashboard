# InsForge Configuration
# Get these values from your InsForge dashboard: https://insforge.dev

# Your InsForge backend URL (required for authentication)
NEXT_PUBLIC_INSFORGE_BASE_URL=https://your-project.insforge.app

# Anonymous key (only needed for database/storage operations)
NEXT_PUBLIC_INSFORGE_ANON_KEY=your_anon_key_here

# Python RAG Service URL (for Next.js frontend to call)
NEXT_PUBLIC_PYTHON_API_URL=http://localhost:8000

# DashScope API Key (for LLM, Embedding, Rerank)
DASHSCOPE_API_KEY=sk-your_dashscope_api_key

# DashVector Configuration (for vector search)
DASHVECTOR_API_KEY=your_dashvector_api_key
DASHVECTOR_HOTEL_ENDPOINT=your_dashvector_endpoint
